<mvc:View controllerName="project1.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
		<subHeader>
			<OverflowToolbar>
				<SearchField liveChange="onSearch" />
			</OverflowToolbar>
		</subHeader>
        <List
		items="{global>/Invoices}"
		headerText="Invoices"
		id ="idList">
		<ObjectListItem
			title="{global>vendorCode}"
			intro="{global>invoiceNumber}"
			number="{global>seqNo}"
			numberUnit="Comp.code {global>companyCode}"
			type="Active"
			press="onListItemPress">
		</ObjectListItem>
	</List>
    </Page>
</mvc:View>